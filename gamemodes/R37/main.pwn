	Iter_Remove(iterPlayerIgnore[playerid], player);

	format(gstr, sizeof(gstr), "[IGNORE] %s(%i) is not ignored by you anymore.", __GetName(player), player);
	SCM(playerid, YELLOW, gstr);
	return true;
}

