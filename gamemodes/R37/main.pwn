YCMD:avgping(playerid, params[], help)
{
	new pings, players;
	
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
	    if(IsPlayerConnected(i))
	    {
			players++;
			pings += GetPlayerPing(i);
	    }
