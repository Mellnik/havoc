					GZoneData[r][e_timeleft] = 0;
					GZoneData[r][e_attacker] = 0;
					GZoneData[r][e_defender] = 0;
					GZoneData[r][e_underattack] = false;
					GZoneData[r][e_locked] = gettime() + 7200;
				}
				
				orm_update(GZoneData[r][e_ormid]);
			}
