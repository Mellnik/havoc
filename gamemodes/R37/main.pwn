				mysql_format(pSQL, gstr2, sizeof(gstr2), "SELECT `id` FROM `logonlog` WHERE `id` = %i AND `ip` = '%e' AND `service` = 'SERVICE_SERVER' AND `action` = 'ACTION_LOGIN' ORDER BY `date` DESC LIMIT 1;",
