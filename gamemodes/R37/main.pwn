			SCM(playerid, COLOR_STEELBLUE, gstr);
		}
		else
		{
            SCM(playerid, -1, ""er"Player is not available!");
		}
	}
	else
	{
		SCM(playerid, -1, NO_PERM);
	}
	return 1;
}

