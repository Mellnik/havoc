    }
    else
    {
        	SetSpawnInfoEx(playerid, NO_TEAM, PlayerSettings[playerid][e_skin] != -1 ? PlayerSettings[playerid][e_skin] : GetPlayerSkin(playerid),
			HouseData[PlayerData[playerid][e_iHouseIdForSpawn]][e_pos][0],
