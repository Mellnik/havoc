			SCMToAll(COLOR_STEELBLUE, gstr);
		}
		else
		{
            SCM(playerid, -1, ""er"Player is not available or yourself");
		}
	}
	else
	{
		SCM(playerid, -1, NO_PERM);
	}
	return true;
}

